<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>netstandard2.1;net5.0</TargetFrameworks>
    <PackageId>Oak.TaskScheduler</PackageId>
    <Title>Oak.TaskScheduler</Title>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Authors>Chris (@chrisOaker1)</Authors>
    <Company>-</Company>
    <PackageProjectUrl>https://github.com/Oakc/TaskScheduler</PackageProjectUrl>
    <Description></Description>
    <Summary></Summary>
    <RepositoryUrl>https://github.com/Oakc/TaskScheduler</RepositoryUrl>
    <PackageTags>tasks schedule cron</PackageTags>
    <PackageReleaseNotes></PackageReleaseNotes>
    <AssemblyName>Oak.TaskScheduler</AssemblyName>
    <PackageIcon></PackageIcon>
    <PublishRepositoryUrl>true</PublishRepositoryUrl>
    <EmbedUntrackedSources>true</EmbedUntrackedSources>
    <AllowedOutputExtensionsInPackageBuildOutputFolder>$(AllowedOutputExtensionsInPackageBuildOutputFolder);.pdb</AllowedOutputExtensionsInPackageBuildOutputFolder>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.Hosting" Version="5.0.0" />
  </ItemGroup>
</Project>
